                Content="/"
                HorizontalAlignment="Left"
                Height="56"
                Margin="318,128,0,0" 
                VerticalAlignment="Top"
                Width="62"
                Click="Divide_Click" Background="#FFEFC4F3"/>
        <Button x:Name="zero"
                Content="0"
                HorizontalAlignment="Left" 
                Height="56"
                Margin="50,445,0,0"
                VerticalAlignment="Top" 
                Width="148" Click="Zero_Click"/>
        <Button x:Name="dot" 
                Content="." 
                HorizontalAlignment="Left"
                Height="56"
                Margin="227,445,0,0" 
                VerticalAlignment="Top"
                Width="62"
                Click="Dot_Click" Background="#FFFCBFD5"/>
        <Button x:Name="equal" 
                Content="="
                HorizontalAlignment="Left" 
                Height="56"
                Margin="318,445,0,0" 
                VerticalAlignment="Top"
                Width="62" 
                Click="Equal_Click" Background="#FFD3A6C0"/>
    </Grid>
</Window>
